let n = 0;
function countUp() {
    if (n==10) {
        alert("You made it to " + n);
    }
    
    else {
        n += 1;
        document.getElementById('counter').innerHTML = 'Counter: ' + n;
    }

}

function countReset() {
    n = 0;
    document.getElementById('counter').innerHTML = 'Counter: ' + n;
}

function countDown() {
    n -= 1;
    document.getElementById('counter').innerHTML = 'Counter: ' + n;
}





